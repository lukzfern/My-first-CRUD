{
    "name": "tennis-app",
    "version": "1.0.0",
    "description": "Simple website for tennis matches",
    "main": "index.js",
    "scripts": {
        "start": "npm-run-all -l -c -s webpack:dev",
        "dev:mock": "npm-run-all -l -c -p server:mock webpack:dev",
        "webpack:dev": "webpack-cli serve --mode development",
        "server:mock": "nodemon server/server.js",
        "build": "webpack --mode production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lukzfern/tennis-app.git"
    },
    "author": "Lucas Fernandez",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/lukzfern/tennis-app/issues"
    },
    "homepage": "https://github.com/lukzfern/tennis-app#readme",
    "dependencies": {
        "axios": "^0.27.2",
        "bootstrap": "^5.1.3",
        "cors": "^2.8.5",
        "nodemon": "^2.0.16",
        "npm-run-all": "^4.1.5",
        "react": "^18.1.0",
        "react-bootstrap": "^2.4.0",
        "react-dom": "^18.1.0",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "styled-components": "^5.3.5"
    },
    "prettier": {
        "semi": true,
        "tabWidth": 2,
        "trailingComma": "all",
        "singleQuote": true,
