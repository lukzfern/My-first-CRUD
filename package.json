        "css-loader": "^6.7.1",
        "express": "^4.18.1",
        "html-loader": "^3.1.0",
        "html-webpack-plugin": "^5.5.0",
        "prettier": "2.6.2",
        "style-loader": "^3.3.1",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.2"
    }
