    },
    "devDependencies": {
        "@babel/core": "^7.18.2",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
        "babel-loader": "^8.2.5",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "body-parser": "^1.20.0",
